{
  "name": "scrutiny-report-ssr",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/scrutiny-report-ssr/src",
  "projectType": "application",
  "tags": [],
  "// targets": "to see all targets run: nx show project scrutiny-report-ssr --web",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/scrutiny-report-ssr"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "scrutiny-report-ssr:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "scrutiny-report-ssr:build:development",
          "hmr": true
        },
        "production": {
          "buildTarget": "scrutiny-report-ssr:build:production",
          "hmr": false
        }
      }
    },
    "dev": {
      "executor": "@nx/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "scrutiny-report-ssr:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "scrutiny-report-ssr:build:development",
          "hmr": true
        },
        "production": {
          "buildTarget": "scrutiny-report-ssr:build:production",
          "hmr": false
        }
      }
    }
  }
}
